%{
#include <stdio.h>
#include <string.h>

#define YYSTYPE char *
#define YYERROR_VERBOSE
#define YYDEBUG 1

int yylineno = 1;

void
yyerror(const char *str)
{
	fprintf(stderr, "Error: %s at line %d\n", str, yylineno);
}

int
yywrap(void)
{
	return 1;
}

%}

%token INTEGER
%token BOOLEAN
%token STRING
%token QPRINTABLE
%token BASE64
%token Null
%token INVALID
